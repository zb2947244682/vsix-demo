{
  "name": "code-helper-demo",
  "displayName": "代码助手演示",
  "description": "一个简单的VSCode插件演示，包含右键打开Notepad等实用功能",
  "version": "0.2.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/zb2947244682/vsix-demo.git"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "demo.openNotepad",
        "title": "打开 Notepad"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "demo.openNotepad",
          "group": "navigation",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "eslint": "^9.32.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
